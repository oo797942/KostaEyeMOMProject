<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="board">


<select id="allBoard" parameterType="string" resultType="boardVO"> 
	select * from normal_board where b_cate=#{title}
</select>



<!-- 회원가입 sql -->
<insert id="boardInsert" parameterType="memberVO"> <!-- insert,delete,update는 리턴타입 필요없음 -->
	Insert into mom_user(u_id, u_pass, u_name, u_nick, u_tel, u_email, u_addr, u_signup_date) VALUES(#{u_id}, #{u_pass}, #{u_name}, #{u_nick}, #{u_tel}, #{u_email}, #{u_addr}, SYSDATE)
</insert>
<!-- userId 같은 이름들은 parameterClass의 클래스 필드 명 -->
 
</mapper>